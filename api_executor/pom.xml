  <dependencies>
    <dependency>
      <groupId>com.nobodyhub.transcendence</groupId>
      <artifactId>api_common</artifactId>
    </dependency>
    <dependency>
      <groupId>com.nobodyhub.transcendence</groupId>
      <artifactId>common_utils</artifactId>
    </dependency>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-web</artifactId>
